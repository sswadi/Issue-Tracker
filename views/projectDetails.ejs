<html>
<head>
    <title> <%= title %> </title>
    <script src="https://kit.fontawesome.com/34ad640c13.js" crossorigin="anonymous"></script>
</head>
<body>

    <div class="Main-container">

        <div class="filter">

        </div>
        <div class="search">
        

        </div>
        
        <div class="details">
            <ul>
                <% for(let i of create_Proj){ %>
                    <div class="deetsSection">
                        <li class="ProjectList">
                            <p>Name: <%= i.name %></p>
                            <p>Description: <%= i.description %></p>
                            <p>Author: <%= i.author %></p>
                        </li>
                <% } %>
            </ul>

        </div>
        <div id="button" >
            <form action="/createIssue/" method="post">
                <button>
                    Create Issue
                </button>
            </form>
        </div>


    </div>

</body>
</html>